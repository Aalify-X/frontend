{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python@2.0.1"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "FLASK_APP": "app.py",
        "PYTHONUNBUFFERED": "1"
    }
}
