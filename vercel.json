{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "FLASK_APP": "app.py",
        "PYTHONUNBUFFERED": "1"
    }
}
